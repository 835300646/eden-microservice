<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>
    <artifactId>eden-uaa</artifactId>
    <name>uaa</name>
    <description>认证中心</description>

    <parent>
        <groupId>org.ylzl</groupId>
        <artifactId>eden-microservice</artifactId>
        <version>1.0.0.RELEASE</version>
        <relativePath>..</relativePath>
    </parent>

    <properties>
        <server.url>http://localhost:9999</server.url>
		<start-class>org.ylzl.eden.uaa.Application</start-class>
    </properties>

    <profiles>
        <profile>
            <id>liquibase</id>
            <properties>
                <liquibase-maven-plugin.domain>org.ylzl.eden.${project.name}.domain</liquibase-maven-plugin.domain>
            </properties>
			<build>
				<plugins>
					<plugin>
						<groupId>it.ozimov</groupId>
						<artifactId>yaml-properties-maven-plugin</artifactId>
					</plugin>
					<plugin>
						<groupId>org.liquibase</groupId>
						<artifactId>liquibase-maven-plugin</artifactId>
					</plugin>
				</plugins>
			</build>
            <dependencies>
                <dependency>
                    <groupId>org.ylzl</groupId>
                    <artifactId>eden-liquibase-spring-boot-starter</artifactId>
                </dependency>
            </dependencies>
        </profile>
        <profile>
            <id>mysql</id>
            <properties>
                <spring.datasource.driver>com.mysql.jdbc.Driver</spring.datasource.driver>
				<spring.jpa.database>MYSQL</spring.jpa.database>
				<spring.jpa.database-platform>org.hibernate.dialect.MySQLDialect</spring.jpa.database-platform>
            </properties>
            <dependencies>
                <dependency>
                    <groupId>org.ylzl</groupId>
                    <artifactId>eden-mysql-spring-boot-starter</artifactId>
                </dependency>
            </dependencies>
        </profile>
		<profile>
			<id>oracle</id>
			<properties>
				<spring.datasource.driver>oracle.jdbc.driver.OracleDriver</spring.datasource.driver>
				<spring.jpa.database>ORACLE</spring.jpa.database>
				<spring.jpa.database-platform>org.hibernate.dialect.Oracle10gDialect</spring.jpa.database-platform>
			</properties>
			<dependencies>
				<dependency>
					<groupId>org.ylzl</groupId>
					<artifactId>eden-oracle-spring-boot-starter</artifactId>
				</dependency>
			</dependencies>
		</profile>
        <profile>
            <id>rabbitmq</id>
            <dependencies>
                <dependency>
                    <groupId>org.ylzl</groupId>
                    <artifactId>eden-cloud-bus-rabbitmq-spring-boot-starter</artifactId>
                </dependency>
            </dependencies>
        </profile>
        <profile>
            <id>redis</id>
            <dependencies>
                <dependency>
                    <groupId>org.ylzl</groupId>
                    <artifactId>eden-redis-spring-boot-starter</artifactId>
                </dependency>
            </dependencies>
        </profile>
    </profiles>

    <dependencies>
		<dependency>
			<groupId>org.ylzl</groupId>
			<artifactId>eden-cloud-admin-client-spring-boot-starter</artifactId>
		</dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-cloud-config-client-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-eureka-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-hazelcast-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-hibernate-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-hikaricp-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-jpa-spring-boot-starter</artifactId>
        </dependency>
		<dependency>
			<groupId>org.ylzl</groupId>
			<artifactId>eden-liquibase-spring-boot-starter</artifactId>
		</dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-metrics-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-oauth2-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.ylzl</groupId>
            <artifactId>eden-swagger2-spring-boot-starter</artifactId>
        </dependency>
    </dependencies>

</project>
